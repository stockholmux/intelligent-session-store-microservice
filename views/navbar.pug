extends _bootstrap
append styles
	// Custom styles for this template
	link(href='/static/css/navbar.css', rel='stylesheet')
block body
	.container
		// Static navbar
		nav.navbar.navbar-default
			.container-fluid
				.navbar-header
					button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href='#') Smart Sessions Demo
				#navbar.navbar-collapse.collapse
					ul.nav.navbar-nav
						li
							a(href='/') Home
						li
							a(href='/group-notifications') Group Notifications
		if notice
			+alert-info(notice)
		block content
		if links
			.container
				ul
					each link in links
						li
							a(href= link) #{link}
		if sessionInfo.activity
			+panel-info("Session Info")
				.row
					.col-sm-6
						h5 Counts
						div Previously Visted: #{sessionInfo.previouslyVisited}
						div Pages Consumed: #{sessionInfo.totalPages}
						div Unique Pages: #{sessionInfo.uniquePages}
						div Activity This Minute: #{sessionInfo.activityThisMinute}
						div Activity over past 5 minutes: #{sessionInfo.activity}

					
